{
  "expo": {
    "entryPoint": "./App.js",
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "name": "PartyGames",
    "slug": "PartyGames",
    "scheme": "PartyGames",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "newArchEnabled": true
      }
    },
    "web": {
      "favicon": "./assets/Images/beer.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
